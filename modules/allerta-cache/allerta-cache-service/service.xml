<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="it.eng.cache">
	<author>GFAVINI</author>
	<namespace>CACHE</namespace>

	<entity name="Dati" local-service="true" remote-service="true" cache-enabled="false" >

		<!-- PK fields -->

		<column name="idDati" type="String" primary="true"  />
		<column name="dato" type="String" />
		<column name="createDate" type="Date" />

		<!-- Order -->

		<order by="asc">
			<order-column name="dato" />
		</order>

	</entity>
	
	
	
</service-builder>