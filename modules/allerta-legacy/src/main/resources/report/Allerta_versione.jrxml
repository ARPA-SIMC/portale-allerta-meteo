	<queryString>
		<![CDATA[select  a.*, a2.numero as numero2, a2.datainizio as datainizio2, a2.datafine as datafine2, a2.tipoallerta as tipoallerta2, a2.titolo as titolo2, a2.parentid as parentid2, a2.descrizionemeteo as descrizionemeteo2, a2.tendenza as tendenza2, a2.note as note2, a2.riferimenti as riferimenti2, u.firstname as arpanome, u.lastname as arpacognome, u2.firstname as protnome,
u2.lastname as protcognome, u3.firstname as arpanome2, u3.lastname as arpacognome2, u4.firstname as protnome2,
u4.lastname as protcognome2,
(select string_agg(''||aa.statoid,'') as xxx from
 (select areaid,max(statoid) as statoid from allerter_allertastato al
  where al.allertaid=a.allertaid and al.areaid>0 and al.statoid>=0 and al.statoid<=3 group by al.areaid order by al.areaid asc ) aa
) as fen_domani,
(select string_agg(''||aa.statoid,'') as xxx from
 (select areaid,max(statoid) as statoid from allerter_allertastato al
  where al.allertaid=a.allertaid and al.areaid<0 and al.statoid>=0 and al.statoid<=3 group by al.areaid order by al.areaid desc ) aa
) as fen_oggi,
(select string_agg(''||aa.statoid,'') as xxx from
 (select areaid,max(statoid) as statoid from allerter_allertastatoversione al
  where al.allertaid=a.allertaid and al.areaid>0 and al.statoid>=0 and al.statoid<=3 group by al.areaid order by al.areaid asc ) aa
) as temp_domani,
(select string_agg(''||aa.statoid,'') as xxx from
 (select areaid,max(statoid) as statoid from allerter_allertastatoversione al
  where al.allertaid=a.allertaid and al.areaid<0 and al.statoid>=0 and al.statoid<=3 group by al.areaid order by al.areaid desc ) aa
) as temp_oggi,
exists(select * from users_roles rr where rr.userid=u2.userid and
rr.roleid::character varying=(select valore from allerter_allertaparametro where
parametroid='RUOLO_DIRETTORE')) as direttore, exists(select * from users_roles rr where
rr.userid=u.userid and rr.roleid=1126970) as per_referente,
case when false then '' else ('dalle ore ' || to_char(a.datainizio,'HH24:MI')
|| ' di ' || (case
when trim(to_char(a.datainizio, 'Day')) = 'Monday' then 'lunedì '
when trim(to_char(a.datainizio, 'Day')) = 'Tuesday' then 'martedì '
when trim(to_char(a.datainizio, 'Day')) = 'Wednesday' then 'mercoledì '
when trim(to_char(a.datainizio, 'Day')) = 'Thursday' then 'giovedì '
when trim(to_char(a.datainizio, 'Day')) = 'Friday' then 'venerdì '
when trim(to_char(a.datainizio, 'Day')) = 'Saturday' then 'sabato '
when trim(to_char(a.datainizio, 'Day')) = 'Sunday' then 'domenica '
else trim(to_char(a.datainizio, 'Day')) || ' ' end) || to_char(a.datainizio,'DD/MM/YYYY')) end
as intestazioneoggi,
case when false then '' else ('dalle ore 00:00 di ' || (case
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Monday' then 'lunedì '
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Tuesday' then 'martedì '
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Wednesday' then 'mercoledì '
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Thursday' then 'giovedì '
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Friday' then 'venerdì '
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Saturday' then 'sabato '
when trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) = 'Sunday' then 'domenica '
else trim(to_char(a.datainizio::date + '1 day'::interval, 'Day')) || ' ' end) || to_char(a.datainizio::date + '1 day'::interval,'DD/MM/YYYY')) end
as intestazionedomani
from allerter_allerta a left join allerter_allertaversione a2 on a.allertaid=a2.allertaid and a2.fase=1 left join
user_ u on a.utentefirmaarpaid = u.userid left join user_ u2 on a.utentefirmaprotid = u2.userid  left join
user_ u3 on a2.utentefirmaarpaid = u3.userid left join user_ u4 on a2.utentefirmaprotid = u4.userid
where $P{ID_RICHIESTA} = a.allertaid]]>
