	<queryString>
		<![CDATA[select allerter_sms.*, CASE
            WHEN allerter_sms.stato = 0 THEN 'In preparazione'::text
            WHEN allerter_sms.stato = 1 THEN 'Pronto per spedizione'::text
            WHEN allerter_sms.stato = 2 THEN 'In spedizione'::text
            WHEN allerter_sms.stato = 3 THEN 'Spedito'::text
            WHEN allerter_sms.stato = 4 THEN 'Invio riuscito'::text
            WHEN allerter_sms.stato = 5 THEN 'Invio fallito'::text
            WHEN allerter_sms.stato = 6 THEN 'Richiesta notifica in corso'::text
            WHEN allerter_sms.stato = 7 THEN 'Notifica consegna non ricevuta'::text
            WHEN allerter_sms.stato = '-1000'::integer THEN 'Duplicato'::text
            WHEN allerter_sms.stato = '-1001'::integer THEN 'Duplicato (invio riuscito)'::text
            WHEN allerter_sms.stato = '-1002'::integer THEN 'Duplicato (invio fallito)'::text
            ELSE NULL::text
        END AS nomestato from allerter_sms where
($P{TIPO} is null or $P{TIPO}::character varying = '' or $P{TIPO}::character varying = tipo)
AND ($P{SOTTOTIPO} is null or $P{SOTTOTIPO}::character varying = '' or $P{SOTTOTIPO}::character varying = sottotipo)
AND ($P{DESTINATARIO} is null or $P{DESTINATARIO}::character varying = '' or upper(nomedestinatario) like '%' || upper($P{DESTINATARIO}) || '%')
AND ($P{NUMERO} is null or $P{NUMERO}::character varying = '' or upper(numero) like '%' || upper($P{NUMERO}) || '%')
AND ($P{STATO} is null or $P{STATO} = -1 or $P{STATO}::bigint = stato)
AND ($P{DATADA} is null or $P{DATADA}::character varying = '' or datainvio >= $P{DATADA}::timestamp without time zone)
AND ($P{DATAA} is null or $P{DATAA}::character varying = '' or datainvio <= $P{DATAA}::timestamp without time zone)
order by nomedestinatario,datainvio limit 50000]]>
	</queryString>
